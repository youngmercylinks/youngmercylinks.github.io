---
layout: null
---
BEGIN:VCALENDAR
CALSCALE:GREGORIAN
VERSION:2.0
METHOD:PUBLISH
PRODID:-//yml.org.au//Jekyll v{{ jekyll.version }}//EN
BEGIN:VTIMEZONE
TZID:Australia/Melbourne
BEGIN:STANDARD
TZOFFSETFROM:+1100
RRULE:FREQ=YEARLY;BYMONTH=4;BYDAY=1SU
DTSTART:20080406T030000
TZNAME:AEST
TZOFFSETTO:+1000
END:STANDARD
BEGIN:DAYLIGHT
TZOFFSETFROM:+1000
RRULE:FREQ=YEARLY;BYMONTH=10;BYDAY=1SU
DTSTART:20081005T020000
TZNAME:AEDT
TZOFFSETTO:+1100
END:DAYLIGHT
END:VTIMEZONE{% assign events = site.events %}{% for event in events %}
BEGIN:VEVENT
TRANSP:OPAQUE
DTEND;TZID=Australia/Melbourne:{{ event.start | date: "%Y%m%dT%H%M%S" }}
UID:{{ event.url | remove_first: '/' | remove: '.html' | uri_escape }}@yml.org.au
DTSTAMP:{% assign theoffsetArray = site.time | date: '%::z' | split: ':' %}{% assign theoffsetHour = theoffsetArray[0] | times: 3600 %}{% assign theoffsetMinute = theoffsetArray[1] | times: 60 %}{% assign theoffsetSecond = theoffsetArray[2] | times: 1 %}{% assign timeoffset = theoffsetHour | plus: theoffsetMinute | plus: theoffsetSecond %}{% assign utctime = site.time | date: '%s' | minus: timeoffset | date: '%Y%m%dT%H%M%S' %}{{ utctime | append: 'Z' }}
DTSTART;TZID=Australia/Melbourne:{{ event.start | date: '%Y%m%dT%H%M%S' }}
LOCATION:{{ event.location.name | append: ' ' }}{{ event.location.address.street | append: ' ' }}{{ event.location.address.locality | append: ' ' }}{{ event.location.address.region | append: ' ' }}{{ event.location.address.postcode | append: ' ' }}{{ event.location.address.country | append: ' ' }}
SUMMARY:Young Mercy Links: {{ event.title }}
DESCRIPTION:{{ event.excerpt | strip_html | strip_newlines }}
URL:{{ site.url | append: site.baseurl | append: event.url }}
END:VEVENT{% endfor %}
END:VCALENDAR